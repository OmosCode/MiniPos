@model List<Basket>    <!-- Google Font: Source Sans Pro --><link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&amp;display=fallback"><!-- Font Awesome Icons --><link href="~/lib/LTE/plugins/fontawesome-free/css/all.css" rel="stylesheet" /><!-- IonIcons --><link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css"><!-- Theme style --><link rel="stylesheet" href="~/temp/dist/adminlte.min.css"><div class="card-body p-0">    <table class="table table-striped projects">        <thead>            <tr>                <th style="width: 20%">                    Adi                </th>                <th style="width: 20%">                    Adet                </th>                <th style="width: 20%">                    Fiyat                </th>                <th style="width: 20%">                    Toplam Fiyat                </th>            </tr>        </thead>        @foreach (var item in Model)        {<tbody>    <tr>        <td>            <a>                @item.UrunAdi            </a>        </td>        <td>            <a>                @item.Adet            </a>        </td>        <td>            <a>                @item.Fiyat            </a>        </td>        <td>            <a>                @item.ToplamTutar            </a>        </td>        <td>            <a class="btn btn-danger " asp-action="Sil" asp-route-id="@item.ID">                <i class="fas fa-trash">                </i>                Sil            </a>        </td>        <td>            <a class="btn btn-info " asp-controller="Satis" asp-action="SatinAl" asp-route-id="@item.ID">                <i class="fa-solid fa-money-bill-1-wave"></i>                Satin al            </a>        </td>    </tr>    <tr></tbody>}    </table>    <div class="container float-left ">        <p class="fw-bold">@ViewBag.Tutar</p>    </div>    <br />    @if (Model.Count>0)    {        <div class="container ">            <a class="btn btn-info form-control" asp-controller="Satis" asp-action="TumSatis">                Satın Al            </a>        </div>    }</div>